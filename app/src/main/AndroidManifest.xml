<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_android">


    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_android">

        <activity android:name=".activity.MenuActivity"></activity>
        <activity android:name=".activity.SingerActivity"></activity>
        <activity android:name=".activity.SongActivity"></activity>
        <activity android:name=".activity.BarBottomActivity"></activity>
        <activity android:name=".activity.PlayMusicActivity"></activity>
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <service android:name=".activity.PlayMusicActivity"></service>
    </application>

</manifest>